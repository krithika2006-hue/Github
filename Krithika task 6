count":48},{"cell_type":"code","source":"import sqlite3\nimport pandas as pd\nimport numpy as np\nimport matplotlib.pyplot as plt\nfrom sklearn.preprocessing import StandardScaler\nfrom sklearn.model_selection import train_test_split\nfrom sklearn.neighbors import KNeighborsClassifier\nfrom sklearn.metrics import accuracy_score, confusion_matrix, ConfusionMatrixDisplay\nfrom matplotlib.colors import ListedColormap\n\n# Step 1: Load data from SQLite file\nconn = sqlite3.connect('/kaggle/input/iris/database.sqlite')\ndf = pd.read_sql_query(\"SELECT * FROM iris\", conn)\nconn.close()\n\n# Step 2: Prepare features and target\nX = df.iloc[:, :-1].values  # all but last column\ny = df.iloc[:, -1].values   # last column as label\n\n# Normalize features\nscaler = StandardScaler()\nX_scaled = scaler.fit_transform(X)\n\n# For decision boundary (only 2 features)\nX_vis = X_scaled[:, :2]\n\n# Train-test split\nX_train, X_test, y_train, y_test = train_test_split(X_scaled, y, test_size=0.2, random_state=42)\n\n# Step 3 and 4: Try different values of K\nk_values = [1, 3, 5, 7, 9]\nfor k in k_values:\n    knn = KNeighborsClassifier(n_neighbors=k)\n    knn.fit(X_train, y_train)\n    y_pred = knn.predict(X_test)\n    \n    acc = accuracy_score(y_test, y_pred)\n    print(f\"K = {k}, Accuracy = {acc:.2f}\")\n    \n    # Confusion matrix\n    cm = confusion_matrix(y_test, y_pred, labels=np.unique(y))\n    disp = ConfusionMatrixDisplay(confusion_matrix=cm, display_labels=np.unique(y))\n    disp.plot()\n    plt.title(f'Confusion Matrix (K={k})')\n    plt.show()\n\n# Step 5: Decision boundary (only 2 features)\ndef plot_decision_boundary(X, y, k):\n    h = 0.02\n    x_min, x_max = X[:, 0].min() - 1, X[:, 0].max() + 1\n    y_min, y_max = X[:, 1].min() - 1, X[:, 1].max() + 1\n    xx, yy = np.meshgrid(np.arange(x_min, x_max, h),\n                         np.arange(y_min, y_max, h))\n\n    clf = KNeighborsClassifier(n_neighbors=k)\n    clf.fit(X, y)\n    Z = clf.predict(np.c_[xx.ravel(), yy.ravel()])\n    Z = Z.reshape(xx.shape)\n\n    cmap_light = ListedColormap(['#FFAAAA', '#AAFFAA', '#AAAAFF'])\n    cmap_bold = ListedColormap(['#FF0000', '#00FF00', '#0000FF'])\n\n    plt.figure(figsize=(8, 6))\n    plt.contourf(xx, yy, Z, cmap=cmap_light, alpha=0.6)\n    plt.scatter(X[:, 0], X[:, 1], c=pd.factorize(y)[0], cmap=cmap_bold, edgecolor='k', s=50)\n    plt.title(f\"Decision Boundary (K = {k})\")\n    plt.xlabel(\"Feature 1\")\n    plt.ylabel(\"Feature 2\")\n    plt.show()\n\n# Plot for selected K values\nfor k in [1, 5, 9]:\n    plot_decision_boundary(X_vis, y, k)","metadata":{"trusted":true,"execution":{"iopub.status.busy":"2025-06-03T14:49:58.137496Z","iopub.execute_input":"2025-06-03T14:49:58.137909Z","iopub.status.idle":"2025-06-03T14:50:01.235244Z","shell.execute_reply.started":"2025-06-03T14:49:58.137884Z","shell.execute_reply":"2025-06-03T14:50:01.233680Z"}},"outputs":[{"name":"stdout","text":"K = 1, Accuracy = 1.00\n","output_type":"stream"},{"output_type":"display_data","data":{"text/plain":"<Figure size 640x480 with 2 Axes>","image/png":"\n"},"metadata":{}},{"name":"stdout","text":"K = 3, Accuracy = 1.00\n","output_type":"stream"},{"output_type":"display_data","data":{"text/plain":"<Figure size 640x480 with 2 Axes>","image/png":"\n"},"metadata":{}},{"name":"stdout","text":"K = 5, Accuracy = 1.00\n","output_type":"stream"},{"output_type":"display_data","data":{"text/plain":"<Figure size 640x480 with 2 Axes>","image/png":"\n"},"metadata":{}},{"name":"stdout","text":"K = 7, Accuracy = 1.00\n","output_type":"stream"},{"output_type":"display_data","data":{"text/plain":"<Figure size 640x480 with 2 Axes>","image/png":"\n"},"metadata":{}},{"name":"stdout","text":"K = 9, Accuracy = 1.00\n","output_type":"stream"},{"output_type":"display_data","data":{"text/plain":"<Figure size 640x480 with 2 Axes>","image/png":"\n"},"metadata":{}},{"traceback":["\u001b[0;31m---------------------------------------------------------------------------\u001b[0m","\u001b[0;31mT